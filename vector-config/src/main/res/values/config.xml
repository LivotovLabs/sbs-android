    <string name="default_push_gateway_http_url" translatable="false">http://127.0.0.1:5000/_matrix/push/v1/notify</string>
    <!-- Note: pusher_app_id cannot exceed 64 chars -->
    <string name="pusher_app_id" translatable="false">app.safebysafe.v2.android</string>

